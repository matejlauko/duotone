{
  "name": "@duotone/preview",
  "version": "0.0.2",
  "license": "MIT",
  "author": "Matej Lauko <matej@lauko.io>",
  "description": "Generate preview of UI components",
  "repository": {
    "type": "git",
    "url": "https://github.com/matejlauko/duotone.git",
    "directory": "packages/preview"
  },
  "bugs": "https://github.com/matejlauko/duotone/issues",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "source": "./lib/index.ts",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "sideEffects": true,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "echo \"no tests\"",
    "typecheck": "tsc --noEmit"
  },
  "tsup": {
    "entry": [
      "lib/index.ts"
    ],
    "sourcemap": true,
    "clean": true,
    "dts": true,
    "format": [
      "cjs",
      "esm"
    ],
    "external": [
      "react"
    ]
  },
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  },
  "devDependencies": {
    "@types/lodash.merge": "^4.6.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^6.2.2",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.0.0",
    "@stitches/react": "^1.2.8",
    "jotai": "^1.8.0",
    "lodash.merge": "^4.6.2",
    "polished": "^4.2.2"
  }
}
